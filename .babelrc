{
    "presets": [
        "module:metro-react-native-babel-preset",
        ["@babel/preset-env", { "targets": { "node": "current" } }],
        "@babel/preset-react"
    ],
    "plugins": [
      ["module:dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "blocklist": null,
        "allowlist": null,
        "safe": false,
        "allowUndefined": true,
        "verbose": false
      }]
    ] 
}